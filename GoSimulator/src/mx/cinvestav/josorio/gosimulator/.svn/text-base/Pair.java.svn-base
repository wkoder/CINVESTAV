package mx.cinvestav.josorio.gosimulator;

/**
 * Pair of integers.
 * @author Moises Osorio
 */
public class Pair {

	private int a, b;
	
	public Pair(int aa, int bb) {
		a = aa;
		b = bb;
	}
	
	public int getA() {
		return a;
	}
	
	public int getB() {
		return b;
	}
	
	@Override
	public int hashCode() {
		return a * 1337 + b;
	}
	
	@Override
	public String toString() {
		return "(" + a + ", " + b + ")";
	}
}
